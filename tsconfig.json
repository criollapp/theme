{
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@angular/*": ["node_modules/@angular/*"],
      "rxjs/*": ["node_modules/rxjs/*"],
      "@criollapp/*": ["node_modules/@criollapp/*"]
    },
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "typings",
    "strict": true,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es6",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es6",
      "dom"
    ],
    "skipLibCheck": true,
    "types": [],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "noImplicitAny": false
  },
  "typings":"index.ts",
  "files": [
    "public_api.ts",
    "node_modules/zone.js/dist/zone.js.d.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": false,
    "flatModuleOutFile": "ca-theme.js",
    "flatModuleId": "ca-theme"
  },
  "exclude": [
    "node_modules"
  ]
}
